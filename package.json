{
  "name": "hackernews-extension",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel public/index.html",
    "prebuild": "rimraf dist",
    "build": "parcel build public/index.html",
    "compile:watch": "tsc -p . --noEmit --watch",
    "zip:build": "cp public/manifest.json dist; cp public/*.png dist; cp background.js dist; cd dist; zip -r ../dist.zip * -x '*.DS_Store'"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/rebass": "^4.0.4",
    "emotion-theming": "^10.0.27",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rebase": "^0.3.0",
    "rebass": "^4.0.7",
    "timeago.js": "^4.0.2"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
